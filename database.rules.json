{
  "rules": {
    "fingerprint": {
      "events": {
        ".read": "auth != null",
        ".write": true,
        "$eventId": {
          ".validate": "newData.hasChildren(['result', 'id', 'timestamp', 'deviceId'])"
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
